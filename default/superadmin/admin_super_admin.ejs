										<div class="content-area">
											<div class="form-container active" id="form-barangay">
												<form id="registerBarangay">
													<div class="form-row">
														<div class="form-col">
															<label class="required" for="barangayName">Barangay Name</label>
															<input type="text" id="barangayName" placeholder="Enter barangay name" required>
														</div>
														<div class="form-col">
															<label class="required" for="barangayCode">Barangay Code</label>
															<input type="text" id="barangayCode" placeholder="Enter unique code" required>
														</div>
													</div>
													<div class="form-row">
														<div class="form-col">
															<label for="barangayCaptain">Barangay Captain</label>
															<input type="text" id="barangayCaptain" placeholder="Enter captain's name">
														</div>
														<div class="form-col">
															<label for="barangayContact">Contact Number</label>
															<input type="text" id="barangayContact" placeholder="e.g. +63 998 765 4321">
														</div>
													</div>
													<button type="submit" class="btn">
														<svg class="btn-icon" width="20" height="20" viewBox="0 0 20 20" fill="none" xmlns="http://www.w3.org/2000/svg">
															<path d="M10 4.375V15.625" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
															<path d="M4.375 10H15.625" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round" />
														</svg> Register Barangay </button>
												</form>
											</div>
											<div class="form-container" id="form-purok">
												<form id="registerPurok">
													<div class="form-group">
														<label class="required" for="selectBarangay">Select Barangay</label>
														<select id="selectBarangay" required>
															<option value="" disabled selected>-- Select Barangay --</option>
															<option value="1">Barangay 1</option>
															<option value="2">Barangay 2</option>
															<option value="3">Barangay 3</option>
															<option value="4">Barangay 4</option>
															<option value="5">Barangay 5</option>
														</select>
													</div>
													<div class="form-row">
														<div class="form-col">
															<label class="required" for="purokName">Purok Name</label>
															<input type="text" id="purokName" placeholder="Enter purok name" required>
														</div>
														<div class="form-col">
															<label class="required" for="purokNumber">Purok Number</label>
															<input type="text" id="purokNumber" placeholder="Enter purok number" required>
														</div>
													</div>
													<div class="form-group">
														<label for="purokLeader">Purok Leader</label>
														<input type="text" id="purokLeader" placeholder="Enter leader's name">
													</div>
													<button type="submit" class="btn">
